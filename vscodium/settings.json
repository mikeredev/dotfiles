{
    "breadcrumbs.enabled": false,
    "editor.codeActionsOnSave": {},
    "editor.comments.insertSpace": true,
    "editor.cursorBlinking": "smooth",
    "editor.cursorSmoothCaretAnimation": "on",
    "editor.detectIndentation": false,
    "editor.fontFamily": "'Monaco', monospace, 'Inconsolata Nerd Font', 'Font Awesome 6 Brands', 'Font Awesome 6 Free Solid'",
    "editor.fontSize": 12,
    "editor.fontWeight": "normal",
    "editor.fontLigatures": true,
    "editor.formatOnType": true,
    "editor.formatOnSave": false,
    "[python]": {
        "editor.formatOnSave": true,
    },
    "editor.insertSpaces": true,
    "editor.indentSize": 4,
    "editor.letterSpacing": 0.2,
    "editor.lineHeight": 1.6,
    "editor.quickSuggestions": {
        "other": false,
        "comments": false,
        "strings": false
    },
    "editor.smoothScrolling": true,
    "editor.tabSize": 4,
    "editor.tabCompletion": "on",
    "editor.wordSeparators": "`~!@#$%^&*()-=+[{]}\\|;:'\",.<>/?",
    "git.confirmSync": false,
    "security.workspace.trust.untrustedFiles": "open",
    "terminal.integrated.fontFamily": "'JetBrains Mono', monospace, 'Noto Color Emoji', 'Font Awesome 6 Brands', 'Font Awesome 6 Free Solid'",
    "workbench.colorTheme": "Monokai Dimmed",
    "window.menuBarVisibility": "toggle",
    "workbench.statusBar.visible": false,
    "workbench.tree.indent": 20,
    "workbench.tree.renderIndentGuides": "always",
    "workbench.colorCustomizations": {
    "tree.indentGuidesStroke": "#05ef3c",
    },
    "editor.minimap.enabled": false,
    "terminal.integrated.fontSize": 12
}